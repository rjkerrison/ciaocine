<ul class='cinema-list'>
  {{#each cinemas}}
    <li class='cinema'>
      <h2>
        <a href='/cinema/{{_id}}'>
          {{name}}
        </a>
      </h2>
      {{#if @root.isUserLoggedIn}}
        <button data-cinema='{{_id}}' data-liked='{{liked}}'></button>
      {{/if}}
      <p>
        {{address}}
        {{zipcode}}
        {{city}}
      </p>
      <ul class='member-card-list'>
        {{#each member_cards}}
          <li class='member-card' data-code='{{code}}'>
            {{label}}
          </li>
        {{/each}}
      </ul>
    </li>
  {{/each}}
</ul>

<script src='/js/cinema/view.js'></script>